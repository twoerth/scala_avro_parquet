@namespace("com.github.twoerth")
protocol SparkParquetAvro {

	enum AminoAcidType {
		ALIPHATIC,
		HYDROXYL,
		CYCLIC,
		AROMATIC,
		BASIC,
		ACIDIC
	}

	record AminoAcid {
	 	AminoAcidType type;
		string fullName;
		string abbreviation;
		float molecularWeight;
	}

	record Event {
		int id;
		string name;
		string desc;
	}
}